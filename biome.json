{
	"$schema": "./node_modules/@biomejs/biome/configuration_schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"files": {
		"ignore": [
			"node_modules/*",
			"test-results/*",
			"playwright-report/*",
			".temp/*"
		],
		"ignoreUnknown": true
	}
}
